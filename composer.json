{
    "name": "ashampoo/rashicons",
    "description": "Latest v 1.x Heroicons package",
    "keywords": ["Blade", "Heroicons", "Laravel"],
    "homepage": "https://github.com/Ashampoo/rashicons",
    "require": {
        "php": "^8.0",
        "blade-ui-kit/blade-icons": "^1.8.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "BladeUI\\Rashicons\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BladeUI\\Rashicons\\BladeHeroiconsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
